{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:js": "parcel watch ./js/index.js --out-dir ./js --out-file bundle.js",
    "build:js": "parcel build ./js/index.js --out-dir ./js --out-file bundle.js",
    "watch:sass": "parcel watch ./scss/main.scss --out-dir ./css --out-file main.css",
    "build:sass": "parcel build ./scss/main.scss --out-dir ./css --out-file main.css"
  },
  "author": "Vuk Jovanovic",
  "license": "ISC",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.1"
  },
  "postcss": {
		"plugins": {
			"autoprefixer": {
				"overrideBrowserslist": [
					">1%",
					"last 4 versions",
					"Firefox ESR",
					"ie >= 9"
				]
			}
		}
	},
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^9.7.1",
    "bounds.js": "^1.0.4",
    "jump.js": "^1.0.2",
    "postcss-modules": "^1.4.1",
    "typeit": "^6.1.1",
    "underscore": "^1.9.1"
  }
}
